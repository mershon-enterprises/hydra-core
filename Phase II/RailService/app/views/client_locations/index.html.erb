<h1>Listing client_locations</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_locations.each do |client_location| %>
      <tr>
        <td><%= client_location.description %></td>
        <td><%= client_location.client_id %></td>
        <td><%= link_to 'Show', client_location %></td>
        <td><%= link_to 'Edit', edit_client_location_path(client_location) %></td>
        <td><%= link_to 'Destroy', client_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client location', new_client_location_path %>
